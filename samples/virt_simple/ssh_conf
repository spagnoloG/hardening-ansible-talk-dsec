Host ubuntu-server-1
  HostName 192.168.121.20
  User vagrant
  Port 22
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile /home/spagnologasper/Documents/projects/hardening-ansible-talk-dsec/samples/virt_simple/.vagrant/machines/ubuntu-server-1/libvirt/private_key
  IdentitiesOnly yes
  LogLevel FATAL
  PubkeyAcceptedKeyTypes +ssh-rsa
  HostKeyAlgorithms +ssh-rsa

